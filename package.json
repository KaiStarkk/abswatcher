{
  "name": "abswatcher",
  "version": "0.3.0",
  "description": "An external file watcher for Audiobookshelf.",
  "main": "lib/index.js",
  "bin": {
    "abswatcher": "lib/index.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikiher/abswatcher.git"
  },
  "scripts": {
    "start": "node lib/index.js",
    "build": "rollup --config rollup.config.mjs && pkg -d -t node18-win-x64 -o dist/abswatcher.exe dist/index.js"
  },
  "keywords": [
    "audiobookshelf",
    "watcher"
  ],
  "author": "mikiher",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.1",
    "express": "^4.18.2",
    "tracer": "^1.3.0",
    "watcher": "^2.3.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "pkg": "^5.8.1",
    "rollup": "^4.2.0"
  }
}
